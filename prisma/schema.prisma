// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlite"
}

model List {
  id          Int     @id @default(autoincrement())
  name String?
  createdAt DateTime
  updatedAt DateTime
}